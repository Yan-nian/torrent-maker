{
    "current_version": "1.3.0",
    "version_files": [
        {
            "file": "torrent_maker.py",
            "patterns": [
                {
                    "pattern": "版本：(\\d+\\.\\d+\\.\\d+)",
                    "replacement": "版本：{version}"
                },
                {
                    "pattern": "Torrent Maker - 单文件版本 v(\\d+\\.\\d+\\.\\d+)",
                    "replacement": "Torrent Maker - 单文件版本 v{version}"
                },
                {
                    "pattern": "Created by Torrent Maker v(\\d+\\.\\d+\\.\\d+)",
                    "replacement": "Created by Torrent Maker v{version}"
                },
                {
                    "pattern": "🚀 v(\\d+\\.\\d+\\.\\d+) 重大更新:",
                    "replacement": "🚀 v{version} 重大更新:"
                },
                {
                    "pattern": "Torrent Maker v(\\d+\\.\\d+\\.\\d+) - 高性能优化版",
                    "replacement": "Torrent Maker v{version} - 高性能优化版"
                },
                {
                    "pattern": "种子创建器 - v(\\d+\\.\\d+\\.\\d+)性能优化版本",
                    "replacement": "种子创建器 - v{version}性能优化版本"
                }
            ]
        },
        {
            "file": "setup.py",
            "patterns": [
                {
                    "pattern": "version='(\\d+\\.\\d+\\.\\d+)'",
                    "replacement": "version='{version}'"
                }
            ]
        },
        {
            "file": "src/main.py",
            "patterns": [
                {
                    "pattern": "版本：(\\d+\\.\\d+\\.\\d+)",
                    "replacement": "版本：{version}"
                }
            ]
        },
        {
            "file": "README.md",
            "patterns": [
                {
                    "pattern": "# torrent-maker v(\\d+\\.\\d+\\.\\d+)",
                    "replacement": "# torrent-maker v{version}"
                }
            ]
        }
    ],
    "changelog_file": "CHANGELOG.md",
    "release_notes_file": "RELEASE_NOTES.md"
}